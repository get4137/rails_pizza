<h2>New order</h2>
<%= render 'form' %>
<%= link_to 'Back', orders_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Price</th>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th></th>
  </tr>
  <% @orders.each do |order| %>
    <tr>
      <td><%= order.title %></td>
      <td><%= order.price %></td>
      <td><%= order.name %></td>
      <td><%= order.email %></td>
      <td><%= order.phone %></td>
      <td><%= link_to 'Show', order_path(order) %></td>
      <td><%= link_to 'Edit', edit_order_path(order) %></td>
    </tr>
  <% end %>
</table>